    "indent-string": "^5.0.0",
    "is-relative-url": "^4.0.0",
    "lerna": "^8.1.9",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.1",
    "only-allow": "^1.2.1",
    "p-map": "^7.0.2",
    "path-exists-cli": "^2.0.0",
    "prettier": "^3.3.3",
